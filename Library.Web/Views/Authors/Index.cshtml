
@{
    ViewData["Title"] = "Index";
}
@* //estan herdando de layaout *@
<h1>Autores</h1>
<div class="row">
    <div class="table">
        <table class="table-responsive">
            <thead>
                <tr>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Last_Name</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


